{
    "name": "no-skeleton.docpad",
    "version": "0.1.0",
    "description": "New DocPad project without using a skeleton",
    "engines": {
        "node": "0.10",
        "npm": "1.3"
    },
    "dependencies": {
        "bower": "~1",
        "docpad": "~6.54.5",
        "docpad-plugin-eco": "~2.0.2",
        "docpad-plugin-moment": "~2.0.2",
        "docpad-plugin-less": "~2.2.0",
        "docpad-plugin-marked": "~2.1.1",
        "docpad-plugin-sunny": "~2.1.0"
    },
    "devDependencies": {
        "cucumber": "latest"
    },
    "scripts": {
        "deploy": "node_modules/.bin/docpad deploy-sunny --offline",
        "postinstall": "node_modules/.bin/bower install",
        "cuke": "node_modules/.bin/cucumber.js"
    }
}
